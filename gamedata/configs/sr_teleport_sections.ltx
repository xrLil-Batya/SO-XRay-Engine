;============ FOG FOREST ============
[fst_space_restrictor_to_escape_1]
enable = true												;Включен/выключен переход, можно использовать как список условий {+info_fst_space_restrictor_to_escape_1_enabled} true, false
spot = level_changer                                  		;Строка из map_spots.xml для отметки перехода на глобальной карте
text = st_level_changer_to_escape_descr						;Текст, высвечивающийся в диалоговом окне
hint = st_space_restrictor_to_escape_descr					;Описание отметки на карте из ui_st_sr_teleport.xml
path = fst_space_restrictor_to_escape_1_drop                ;Waypoint для телепортации актора при нажатии "Да"
reject_path = fst_level_changer_to_escape_1_reject_way      ;Waypoint для телепортации актора при нажатии "Нет"
autosave = true												;Делать автосейв при нажатии "Да"
reject_on_disabled = true									;Поддержка выключенного перехода
warning = st_level_changer_warning_descr					;Описание при выключенном переходе из ui_st_sr_teleport.xml

[fst_space_restrictor_to_rostok_factory_1]
